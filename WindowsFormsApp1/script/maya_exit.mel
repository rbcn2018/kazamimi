global proc quit(){
    //Query scene changes, return without quitting when canceled.
    if(!saveChanges("")) return;

    //Save prefs
    savePrefs;
    saveToolSettings;
    saveViewportSettings;
    pluginInfo -savePluginPrefs;


    //Kill maya
    $i = `getpid`; //application ID

    system("taskkill /PID "  + $i +" /f");
}
